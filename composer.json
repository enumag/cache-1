{
    "name": "amphp/cache",
    "homepage": "https://github.com/amphp/cache",
    "description": "Caching that doesn't block.",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/amphp/cache/issues",
        "irc": "irc://irc.freenode.org/amphp"
    },
    "authors": [
        {
            "name": "Niklas Keller",
            "email": "me@kelunik.com"
        },
        {
            "name": "Daniel Lowrey",
            "email": "rdlowrey@php.net"
        }
    ],
    "require": {
        "php": ">=7.2",
        "amphp/amp": "dev-ext-async"
    },
    "require-dev": {
        "amphp/php-cs-fixer-config": "dev-master",
        "amphp/phpunit-util": "^1",
        "phpunit/phpunit": "^6"
    },
    "autoload": {
        "psr-4": {
            "Amp\\Cache\\": "lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Amp\\Cache\\Test\\": "test"
        }
    },
    "config": {
        "platform": {
            "php": "7.2.0"
        }
    },
    "scripts": {
        "check": [
            "@cs",
            "@test"
        ],
        "cs": "php-cs-fixer fix -v --diff --dry-run",
        "cs-fix": "php-cs-fixer fix -v --diff"
    }
}
